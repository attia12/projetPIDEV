<!-- Star Rating -->
<input id="rating-input" name="note" type="number" class="rating" min=0 max=5 step=0.5 data-size="md" data-show-clear="false" data-show-caption="true">








<link rel="stylesheet" href="{{ asset ('https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css')}}">
<link rel="stylesheet" href="{{ asset ('https://cdnjs.cloudflare.com/ajax/libs/bootstrap-star-rating/4.0.6/css/star-rating.min.css')}}" />
<script src="{{ asset ('https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js')}}"></script>
<script>
$(function() {
  $('#rating-input').rating({
    hoverOnClear: false,
    theme: 'krajee-fas',
    filledStar: '<i class="fa fa-star"></i>',
    emptyStar: '<i class="fa fa-star-o"></i>'
  });

  $('.star').click(function() {
    $(this).prevAll().addBack().addClass('active');
    $(this).nextAll().removeClass('active');
    $('input[name="note"]').val($(this).data('rating'));
  });
});
</script>

